{% extends 'SyliusWebBundle:Frontend:layout.html.twig' %}

{% block title %}{{ n.title }}{% endblock %}
{% block metaKeywords %}{{ n.keyword }}{% endblock %}
{% block metaDescription %}{{ n.description }}{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" type="text/css" href="{{ asset('js/photoswipe/photoswipe.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('css/custom-photoswipe.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('css/news.css') }}">
{% endblock %}

{% block content %}
    {% include 'SyliusWebBundle:Frontend/News:gallery.html.twig' %}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('js/photoswipe/lib/simple-inheritance.min.js') }}"></script>
    <script src="{{ asset('js/photoswipe/code-photoswipe-1.0.11.js') }}"></script>
    <script src="{{ asset('js/init.gallery.js') }}"></script>
    <script src="{{ asset("js/videoClick.js") }}"></script>
{% endblock %}