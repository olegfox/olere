{% form_theme form 'SyliusWebBundle::forms.html.twig' %}

<fieldset>
    {{ form_row(form.id, {'attr': {'class': 'input-lg'}}) }}
    {{ form_row(form.sub, {'attr': {'class': 'input-lg'}}) }}
    {{ form_row(form.enable, {'attr': {'class': 'input-lg'}}) }}
    {{ form_row(form.typeContent, {'attr': {'class': 'input-lg'}}) }}
    {{ form_row(form.typeMenu, {'attr': {'class': 'input-lg'}}) }}
    {{ form_row(form.position, {'attr': {'class': 'input-lg'}}) }}
    {{ form_row(form.title, {'attr': {'class': 'input-lg'}}) }}
    {{ form_row(form.metaTitle, {'attr': {'class': 'input-lg'}}) }}
    {{ form_row(form.metaKeywords, {'attr': {'class': 'input-lg'}}) }}
    {{ form_row(form.metaDescription, {'attr': {'class': 'input-lg'}}) }}
    {{ form_row(form.body, {'attr': {'class': 'redactor'}}) }}
</fieldset>
{{ form_widget(form._token) }}
