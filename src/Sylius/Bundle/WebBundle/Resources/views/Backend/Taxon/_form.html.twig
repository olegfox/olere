{% form_theme form 'SyliusWebBundle::forms.html.twig' %}

<fieldset>
    {{ form_row(form.name, {'attr': {'class': 'input-lg'}}) }}
    {{ form_row(form.metaTitle, {'attr': {'class': 'input-lg'}}) }}
    {{ form_row(form.metaKeywords, {'attr': {'class': 'input-lg'}}) }}
    {{ form_row(form.metaDescription, {'attr': {'class': 'input-lg'}}) }}
    {{ form_row(form.parent, {'attr': {'class': 'input-lg'} }) }}
    {{ form_row(form.permalink, {'attr': {'class': 'input-lg'} }) }}
    {{ form_row(form.file) }}
    {{ form_row(form.file2) }}
    <br>
    {{ form_row(form.description) }}
</fieldset>

{{ form_rest(form) }}
